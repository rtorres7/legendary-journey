{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node --watch ./bin/www",
    "server": "node --watch ./bin/www",
    "server-dependencies": "npm install",
    "client-dependencies": "npm install --prefix client",
    "install-all-deps": "concurrently \"npm run server-dependencies\" \"npm run client-dependencies\"",
    "client": "cd ./client && npm run serve:low",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.6.0",
    "arxiv-api": "^1.1.1",
    "body-parser": "^1.20.0",
    "concurrently": "^7.2.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.18.2",
    "moment": "^2.29.4",
    "mongoose": "^6.8.2",
    "morgan": "~1.9.1"
  }
}
