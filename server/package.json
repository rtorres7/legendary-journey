{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon src/swagger.js",
    "server-dependencies": "npm install",
    "test": "node --experimental-vm-modules node_modules/.bin/jest -i --coverage --runInBand",
    "test:watch": "jest --watch",
    "lint": "eslint --fix src",
    "swagger-autogen": "DOTENV=swagger node src/swagger.js",
    "clean": "rimraf coverage dist tmp",
    "build": "tsc --project tsconfig.json",
    "build:watch": "tsc --watch --project tsconfig.json",
    "build:release": "npm run clean && npm run copy-assets && tsc --project tsconfig.release.json",
    "copy-assets": "copyfiles --all --exclude \"./**/*.{ts,tsx,js,jsx}\" \"./src/**/*\" \"./env/**/*\" ./dist"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/__utils__/testSetup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__utils__",
      "<rootDir>/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "src/app.js",
      "src/mongo/init-db.d/seed.js",
      "src/postgres/seed.js"
    ]
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.6.0",
    "@kiwiproject/kiwi-js": "^0.6.1",
    "cache-manager": "^5.2.2",
    "connect-mongo": "^5.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.3.1",
    "express": "~4.18.2",
    "express-session": "^1.17.3",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "minio": "^7.1.1",
    "mongoose": "^6.8.2",
    "morgan": "~1.9.1",
    "passport": "^0.6.0",
    "passport-oauth2": "^1.7.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.1",
    "swagger-autogen": "2.23.4",
    "swagger-ui-express": "4.6.3",
    "uuid": "^9.0.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.17",
    "@types/jest": "^29.5.3",
    "@types/morgan": "1.9.4",
    "@types/node": "20.4.2",
    "@types/swagger-ui-express": "4.1.3",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "jest": "29.6.1",
    "jest-extended": "4.0.0",
    "nodemon": "^3.0.1",
    "prettier": "3.0.0",
    "rimraf": "~5.0",
    "supertest": "6.3.3",
    "testcontainers": "9.9.1",
    "tmp": "^0.2.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.14.2",
    "typedoc": "^0.24.8",
    "typescript": "^4.2.4"
  },
  "prettier": {
    "editorconfig": true
  }
}
