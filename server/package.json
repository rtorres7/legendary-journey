{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node --inspect=0.0.0.0 --watch src/bin/www",
    "server-dependencies": "npm install",
    "test": "node --experimental-vm-modules node_modules/.bin/jest -i --coverage"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/__utils__/testSetup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__utils__",
      "<rootDir>/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "src/app.js",
      "src/postgres/seed.js"
    ]
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.6.0",
    "@kiwiproject/kiwi-js": "^0.6.1",
    "cache-manager": "^5.2.2",
    "connect-mongo": "^5.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "express": "~4.18.2",
    "express-session": "^1.17.3",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "minio": "^7.1.1",
    "mongoose": "^6.8.2",
    "morgan": "~1.9.1",
    "passport": "^0.6.0",
    "passport-oauth2": "^1.7.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.1",
    "swagger-jsdoc": "^6.0.8",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^9.0.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "jest-extended": "^4.0.0",
    "supertest": "^6.3.3",
    "testcontainers": "^9.8.0"
  }
}
