{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node --watch src/bin/www",
    "server-dependencies": "npm install",
    "test": "node --experimental-vm-modules node_modules/.bin/jest -i --coverage --collectCoverageFrom=\"src/**/{!(app),}.js\""
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/__utils__/testSetup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__utils__",
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.6.0",
    "@kiwiproject/kiwi-js": "^0.6.1",
    "body-parser": "^1.20.0",
    "cache-manager": "^5.2.2",
    "concurrently": "^7.2.1",
    "connect-mongo": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "debug": "~2.6.9",
    "dotenv": "^16.1.4",
    "express": "~4.18.2",
    "express-jwt": "^8.4.1",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.2",
    "morgan": "~1.9.1",
    "openid-client": "^5.4.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.7.0",
    "passport-openidconnect": "^0.1.1",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.1",
    "session": "^0.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "jest-extended": "^4.0.0",
    "supertest": "^6.3.3",
    "testcontainers": "^9.8.0"
  }
}
