{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node --watch src/swagger.js",
    "server-dependencies": "npm install",
    "test": "node --experimental-vm-modules node_modules/.bin/jest -i --coverage",
    "lint": "eslint --ext .js --ignore-path .gitignore --fix src",
    "swagger-autogen": "POSTGRES_CONNECTION_URL=SWAGGER MXS_OAUTH_ID=maximus ES_URL=SWAGGER MONGO_DATABASE_URL=SWAGGER node src/swagger.js"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/__utils__/testSetup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__utils__",
      "<rootDir>/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "src/app.js",
      "src/postgres/seed.js"
    ]
  },
  "dependencies": {
    "@elastic/elasticsearch": "8.8.1",
    "@kiwiproject/kiwi-js": "0.7.0",
    "cache-manager": "5.2.3",
    "connect-mongo": "5.0.0",
    "cors": "2.8.5",
    "dayjs": "1.11.9",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "jsonwebtoken": "9.0.1",
    "mongoose": "7.3.2",
    "morgan": "1.10.0",
    "passport": "0.6.0",
    "passport-oauth2": "1.7.0",
    "pg": "8.11.1",
    "pg-hstore": "2.3.4",
    "sequelize": "6.32.1",
    "swagger-autogen": "2.23.4",
    "swagger-ui-express": "4.6.3",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "jest": "29.6.1",
    "jest-extended": "4.0.0",
    "prettier": "3.0.0",
    "supertest": "6.3.3",
    "testcontainers": "9.9.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ]
  }
}
