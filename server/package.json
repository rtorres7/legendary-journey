{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start": "node  --experimental-vm-modules --watch --require ts-node/register src/swagger.ts",
    "start:inspect": "node  --experimental-vm-modules --watch --require ts-node/register --inspect=0.0.0.0:9229 src/swagger.ts",
    "server-dependencies": "npm install",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --coverage --runInBand --detectOpenHandles",
    "testone": "node --experimental-vm-modules node_modules/.bin/jest --coverage --runInBand --detectOpenHandles --verbose=true",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "lint": "eslint --ignore-path .gitignore --fix src",
    "swagger-autogen": "DOTENV=swagger node src/swagger.js",
    "clean": "rimraf build coverage dist tmp",
    "build": "tsc --project tsconfig.json",
    "build:watch": "tsc --watch --project tsconfig.json",
    "build:release": "npm run clean && npm run copy-assets && tsc --project tsconfig.release.json",
    "copy-assets": "copyfiles --all --exclude \"./**/*.{ts,tsx,js,jsx}\" \"./src/**/*\" \"./env/**/*\" ./build",
    "delay": "node -e \"setTimeout(() => process.exit(0), 5000)\"",
    "prestart:tsc": "npm run clean && npm run copy-assets && npm run build",
    "start:tsc": "concurrently --names \"build,node\" \"npm:build:watch\" \"npm run delay && nodemon build/src/swagger.js\"",
    "start:ts-node": "npm run clean && ts-node src/swagger.js"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "node",
    "verbose": true,
    "roots": [
      "__tests__"
    ],
    "reporters": [
      [
        "summary",
        {
          "summaryThreshold": 0
        }
      ]
    ],
    "globalSetup": "<rootDir>/__tests__/__utils__/global-setup.ts",
    "globalTeardown": "<rootDir>/__tests__/__utils__/global-teardown.ts",
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__utils__",
      "<rootDir>/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "src/app.js",
      "src/mongo/init-db.d/seed.js",
      "src/postgres/seed.js",
      "src/swagger.js"
    ]
  },
  "dependencies": {
    "@elastic/elasticsearch": "8.8.1",
    "@kiwiproject/kiwi-js": "0.7.0",
    "cache-manager": "5.2.3",
    "connect-mongo": "5.0.0",
    "cors": "2.8.5",
    "dayjs": "1.11.9",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "joi": "17.9.2",
    "jsonwebtoken": "9.0.1",
    "lodash": "4.17.21",
    "minio": "7.1.1",
    "mongoose": "7.4.0",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "passport": "0.6.0",
    "passport-oauth2": "1.7.0",
    "pg": "8.11.1",
    "pg-hstore": "2.3.4",
    "sequelize": "6.32.1",
    "swagger-autogen": "2.23.5",
    "swagger-ui-express": "5.0.0",
    "uuid": "9.0.0",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "@kiwiproject/kiwi-test-js": "0.1.1",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.3",
    "@types/lodash": "4.14.195",
    "@types/morgan": "1.9.4",
    "@types/multer": "1.4.7",
    "@types/node": "20.4.2",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "@types/tmp": "0.2.3",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "concurrently": "8.2.0",
    "copyfiles": "2.4.1",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "jest": "29.6.1",
    "jest-extended": "4.0.0",
    "nodemon": "3.0.1",
    "prettier": "3.0.0",
    "rimraf": "5.0",
    "supertest": "6.3.3",
    "testcontainers": "9.11.0",
    "tmp": "0.2.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typedoc": "0.24.8",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": "^18 || ^20",
    "npm": ">= 6.13.4"
  },
  "prettier": {
    "editorconfig": true
  }
}
