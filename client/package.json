{
  "name": "max-ui",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_ENV=high vue-cli-service serve --mode high",
    "build": "cross-env NODE_ENV=high vue-cli-service build --mode high",
    "serve:offline": "cross-env NODE_ENV=offline vue-cli-service serve --mode offline",
    "build:offline": "cross-env NODE_ENV=offline vue-cli-service build --mode offline",
    "serve:container": "cross-env NODE_ENV=development vue-cli-service serve",
    "build:container": "cross-env NODE_ENV=development vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:ci": "vue-cli-service lint --format json --output-file eslint_report.json"
  },
  "dependencies": {
    "@amcharts/amcharts5": "^5.2.4",
    "@amcharts/amcharts5-geodata": "^5.0.3",
    "@ckeditor/ckeditor5-basic-styles": "^35.3.2",
    "@ckeditor/ckeditor5-dev-utils": "^31.1.13",
    "@ckeditor/ckeditor5-dev-webpack-plugin": "^31.1.13",
    "@ckeditor/ckeditor5-editor-classic": "^35.3.2",
    "@ckeditor/ckeditor5-essentials": "^35.3.2",
    "@ckeditor/ckeditor5-font": "^35.3.2",
    "@ckeditor/ckeditor5-heading": "^35.3.2",
    "@ckeditor/ckeditor5-html-support": "^35.3.2",
    "@ckeditor/ckeditor5-image": "^35.3.2",
    "@ckeditor/ckeditor5-indent": "^35.3.2",
    "@ckeditor/ckeditor5-link": "^35.3.2",
    "@ckeditor/ckeditor5-list": "^35.3.2",
    "@ckeditor/ckeditor5-paragraph": "^35.3.2",
    "@ckeditor/ckeditor5-style": "^35.3.2",
    "@ckeditor/ckeditor5-theme-lark": "^35.3.2",
    "@ckeditor/ckeditor5-upload": "^35.3.2",
    "@ckeditor/ckeditor5-vue": "^4.0.1",
    "@headlessui/vue": "^1.6.7",
    "@heroicons/vue": "2.0.13",
    "@vuepic/vue-datepicker": "^3.5.2",
    "axios": "^0.26.1",
    "core-js": "^3.8.3",
    "cypress": "^12.5.1",
    "dayjs": "^1.11.1",
    "inter-ui": "^3.19.3",
    "lodash": "^4.17.21",
    "postcss-loader": "^4.0.0",
    "raw-loader": "^4.0.2",
    "vee-validate": "^4.5.8",
    "vue": "^3.2.13",
    "vue-cookies": "^1.8.3",
    "vue-router": "^4.0.3",
    "vue-tippy": "^6.0.0",
    "vue3-carousel": "^0.2.9",
    "vue3-cookies": "^1.0.6",
    "vue3-markdown-it": "^1.0.10",
    "vue3-simple-typeahead": "^1.0.11",
    "vuex": "^4.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.0.3",
    "postcss": "^8.4.12",
    "prettier": "2.8.0",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "tailwindcss": "^3.0.23"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "vue/setup-compiler-macros": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/base",
      "plugin:vue/vue3-essential",
      "plugin:vue/vue3-recommended",
      "plugin:vue/vue3-strongly-recommended",
      "prettier"
    ],
    "ignorePatterns": [
      "*.cy.js"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/max-attributes-per-line": [
        "error",
        {
          "singleline": {
            "max": 4
          },
          "multiline": {
            "max": 4
          }
        }
      ],
      "vue/first-attribute-linebreak": "off",
      "vue/v-on-event-hyphenation": "off",
      "vue/attribute-hyphenation": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
